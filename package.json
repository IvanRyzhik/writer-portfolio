{
  "name": "writer-portfolio-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PRIVATE_SUPPRESS_WARNINGS=true next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy"
  },
  "dependencies": {
    "flowbite": "^3.1.2",
    "flowbite-react": "^0.12.7",
    "framer-motion": "latest",
    "init": "^0.1.2",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-modal": "latest",
    "react-slick": "latest",
    "slick-carousel": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.15",
    "@types/node": "24.2.1",
    "@types/react": "19.1.9",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "vercel": "^47.0.1",
    "wrangler": "^4.33.2"
  }
